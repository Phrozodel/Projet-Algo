WITH Ada.Text_IO,Ada.Integer_Text_IO,Outils,Gestion_Guildes,Gestion_Coachs,Gestion_Poke;
USE Ada.Text_IO,Ada.Integer_Text_IO,Outils,Gestion_Guildes,Gestion_Coachs,
   Gestion_Poke;

PROCEDURE Principal IS
   G       : T_Guilde;
   Liste_G : T_Ptg;
   C: t_coach;
   Liste_C : T_PtC;
BEGIN
   Put_Line("bienvenu");
   Newguilde(G);
   Insertion_ListeG (
      Liste_G,G);
   Visug(Liste_G);
--   Dissolution(
--      Liste_G,G);
--   Visug(Liste_G);
NewCoach (C);
Insertion_ListeC (Liste_C,C );
VisuCoach (Liste_C);


END Principal;
